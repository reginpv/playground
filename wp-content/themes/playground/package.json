  "scripts": {
    "watch": "npx tailwindcss -i src/scss/app.scss -o dist/main.css --postcss --sass --watch",
    "build": "npx tailwindcss build -i src/scss/app.scss -o dist/main.css",
    "build:minify": "postcss dist/main.css -o dist/main.css --use cssnano"
  }
